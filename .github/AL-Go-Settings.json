  "DeployToDVPMS": {
    "EnvironmentName": "Sandbox_NTN10",
    "continuousDeployment": true
  },
  "DeployToPROD": {
    "EnvironmentName": "Production",
    "continuousDeployment": false
  },
  "workflows": [ "Current" ],
        "settings": {
            "workflowSchedule": {
                "cron": "0 2 * * *"
